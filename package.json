{
  "name": "coffee-shop-api",
  "version": "1.0.0",
  "description": "Serverless Coffee Shop REST API with AWS API Gateway, Lambda, and DynamoDB",
  "main": "handler.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy:dev": "serverless deploy --stage dev",
